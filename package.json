{
  "name": "marine-licensing-frontend",
  "version": "0.0.0",
  "description": "CDP Frontend Template",
  "sideEffects": false,
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.16.0"
  },
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "npm run build:frontend",
    "build:frontend": "NODE_ENV=production webpack",
    "dev": "run-p frontend:watch server:watch",
    "dev:debug": "run-p frontend:watch server:debug",
    "format": "prettier --write \"src/**/*.js\" \"**/*.{js,cjs,md,json,config.js,test.js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"**/*.{js,cjs,md,json,config.js,test.js}\"",
    "frontend:watch": "NODE_ENV=development webpack --watch",
    "lint": "run-s lint:js lint:scss",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint .",
    "lint:scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "test": "TZ=UTC vitest run --coverage",
    "test:watch": "TZ=UTC vitest",
    "test:integration:allure": "npx vitest run tests/ --coverage=false && npx allure generate allure-results --single-file --clean --name marine-licensing-dom-testing-library-tests",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict .",
    "knip": "knip"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/hapi-secure-context": "^0.3.0",
    "@defra/hapi-tracing": "^1.28.0",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/basic": "^7.0.2",
    "@hapi/bell": "^13.1.0",
    "@hapi/catbox-memory": "^6.0.2",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^21.4.3",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.1",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@hapi/yar": "^11.0.3",
    "@microsoft/clarity": "^1.0.0",
    "aws-embedded-metrics": "^4.2.0",
    "convict": "^6.2.4",
    "cssnano": "^7.1.0",
    "cssnano-preset-default": "^7.0.9",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "global-agent": "^3.0.0",
    "govuk-frontend": "^5.12.0",
    "hapi-pino": "^13.0.0",
    "hapi-pulse": "^3.0.1",
    "ioredis": "^5.8.1",
    "joi": "^18.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "nunjucks": "^3.2.4",
    "ol": "^10.6.1",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "proj4": "^2.19.10",
    "rfc2047": "^4.0.1",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@babel/preset-env": "^7.26.0",
    "@faker-js/faker": "^9.9.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/runner": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "allure-commandline": "^2.34.1",
    "allure-vitest": "^3.4.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "cheerio": "^1.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.36.0",
    "jsdom": "^25.0.1",
    "knip": "^5.66.2",
    "neostandard": "^0.12.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "stylelint": "^16.24.0",
    "stylelint-config-gds": "^2.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "vitest-fetch-mock": "^0.4.5",
    "webpack": "^5.101.3",
    "webpack-assets-manifest": "6.2.1",
    "webpack-cli": "6.0.1"
  }
}
